{
  "Frontmatter created at": {
    "scope": "markdown",
    "prefix": "xca",
    "body": "---\ncreatedAt: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}T${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}\n---\n"
  },
  "Em dash": {
    "scope": "markdown",
    "prefix": "md",
    "body": "—"
  },
  "Right arrow": {
    "scope": "markdown",
    "prefix": "ra",
    "body": "→"
  }
}
