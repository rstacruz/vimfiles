{
  "Frontmatter created at": {
    "scope": "markdown",
    "prefix": "fca",
    "body": "---\ncreatedAt: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}T${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}\n---\n"
  },
  "Inline created at": {
    "scope": "markdown",
    "prefix": "ica",
    "body": "[+](#CreatedAt:${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}T${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND})"
  },
  "Em dash": {
    "scope": "markdown",
    "prefix": "mdash",
    "body": "—"
  },
  "Right arrow": {
    "scope": "markdown",
    "prefix": "rarr",
    "body": "→"
  }
}
